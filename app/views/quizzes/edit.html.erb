
<h2>Edit <%= @quiz.title %></h2>
<% @quiz.questions.each do |q| %>
<li><%= link_to"#{q.content}", question_path(q) %></li>
<% end %>
<br>

<h3>Edit Quiz Title:</h3>

<%= form_for(@quiz) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <br>

  <%= f.submit %>
<% end %>

<%= link_to 'Edit Questions', questions_path %>
<%= link_to 'Delete Quiz', quiz_path(@quiz),method: 'DELETE' %>
